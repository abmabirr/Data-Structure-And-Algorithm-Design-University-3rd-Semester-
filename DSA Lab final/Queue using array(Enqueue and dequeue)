#include <iostream>
using namespace std;

#define MAX 5

int queue[MAX];
int front = -1, rear = -1;
void enqueue(int value) {
    if (rear == MAX - 1) {
        cout << "Queue Overflow! Cannot enqueue.\n";
    } 
    else {
        if (front == -1)   // First insertion
            front = 0;

        rear++;
        queue[rear] = value;
        cout << value << " enqueued into queue.\n";
    }
}
void dequeue() {
    if (front == -1 || front > rear) {
        cout << "Queue Underflow! Cannot dequeue.\n";
    } 
    else {
        cout << queue[front] << " dequeued from queue.\n";
        front++;
    }
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);

    dequeue();
    dequeue();
    dequeue();
    dequeue();  

    return 0;
}